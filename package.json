    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.22",
    "@types/react-google-recaptcha": "^2.1.8",
    "@types/react-lazy-load-image-component": "^1.6.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "cypress": "^13.3.1",
