    "lucide-react": "^0.456.0",
    "minio": "^7.1.3",
    "moment": "^2.29.4",
    "mongoose": "^8.1.0",
    "mssql": "^10.0.1",
    "nanoid": "^5.0.2",
    "next": "^13.5.4",
    "next-seo": "^6.1.0",
    "next-sitemap": "^4.2.3",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.46.2",
