    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "react-toastify": "^9.1.3",
    "react-tooltip": "^5.26.3",
    "redux-persist": "^6.0.0",
    "swiper": "^10.3.1",
    "tailwindcss": "^3.3.3",
    "universal-cookie": "^6.1.1",
    "xlsx": "^0.18.5",
    "yup": "^1.3.2",
    "zod": "^3.22.2"
