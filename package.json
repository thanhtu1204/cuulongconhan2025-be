    "@reduxjs/toolkit": "^1.9.7",
    "@svgr/webpack": "^8.1.0",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/iconv-lite": "^0.0.1",
    "aos": "^3.0.0-beta.6",
    "aws-sdk": "^2.1545.0",
    "axios": "^1.5.1",
    "classnames": "^2.4.0",
    "cloudinary": "^1.41.0",
    "cookie": "^0.5.0",
    "date-fns": "^2.30.0",
    "file-type": "^19.0.0",
    "flowbite": "^2.0.0",
    "flowbite-react": "^0.7.2",
    "formidable": "v3",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.4",
